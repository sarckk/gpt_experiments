{
    "name": "Boomark List",
    "description": "A Chrome extension that displays a list of bookmarks.",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {},
    "permissions": [
        "activeTab",
        "bookmarks"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}